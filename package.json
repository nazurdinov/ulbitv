{
  "name": "ulbitv",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:web": "webpack serve --env port=3000",
    "start:dev:server": "node ./json-server/index.js",
    "start:devenv": "concurrently \"npm run start:web\" \"npm run start:dev:server\"",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "npx eslint '**/*.{ts,tsx}'",
    "lint:ts:fix": "npx eslint '**/*.{ts,tsx}' --fix",
    "stylelint:scss": "npx stylelint '**/*.scss'",
    "stylelint:scss:fix": "npx stylelint '**/*.scss' --fix",
    "test:unit": "npx jest --config ./config/jest/jest.config.ts",
    "storybook:run": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "test:prepare": "npx loki update --config ./config/storybook",
    "test:ui": "npx loki test --config ./config/storybook",
    "test:ui:ok": "npx loki approve --config ./config/storybook",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "husky:prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
