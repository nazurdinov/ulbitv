    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.15.0",
    "ts-loader": "^9.4.4",
    "webpack-cli": "^5.1.4"
  },
